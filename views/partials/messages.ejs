<% if(typeof errors!= 'undefined') { %>
    <%    errors.forEach(function(error){ %>
            <p id="error_msg1"> <%= error.msg %></p>
    <%    })        %>
    <% } %>
    
    <% if(success_msg!= '') { %>
            <p id="success_msg"><%=success_msg %> </p>
          <% } %>
    
    <% if(error_msg != '') { %>
          <p id="error_msg2"><%= error_msg %></p> 
          <% } %> 
    <% if(error != '') { %>
           <p id="error_msg3"><%= error %> </p>
          <% } %> 